{
  "name": "search-query-builder",
  "version": "1.0.0",
  "description": "Search query builder with quotes, stop words, and utterance matching.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "dev:node": "tsx examples/node-demo.ts",
    "dev:node-1": "tsx examples/node-demo.ts",
    "gen:data": "tsx scripts/generate-demo-data.ts",
    "bench": "tsx scripts/perf-bench.ts",
    "scenarios": "tsx scripts/scenario-runner.ts",
    "preview:browser": "npx http-server . -p 8080 -c-1 --open examples/browser-demo.html"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "http-server": "^14.1.1",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "license": "MIT",
  "dependencies": {
    "fuse.js": "^7.1.0",
    "lodash": "^4.17.21"
  }
}
